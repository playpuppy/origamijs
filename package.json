    "test": "jest"
  },
  "keywords": [
    "Puppy",
    "Transpiler"
  ],
  "author": "Kimio Kuramitsu",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/node": "^12.7.12",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
