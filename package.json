    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/playpuppy/transpiler.git"
  },
  "bugs": {
    "url": "https://github.com/playpuppy/transpiler/issues"
  },
  "homepage": "https://github.com/playpuppy/transpiler"
